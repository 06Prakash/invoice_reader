{
    "UMRN": "UMRN\\s*[:\\s]*([A-Z0-9]+)",
    "Date": "Date\\s*[:\\s]*([\\d/]+)",
    "Sponsor Bank Code": "Sponsor Bank Code\\s*[:\\s]*([A-Z0-9]+)",
    "Utility Code": "Utility Code\\s*[:\\s]*([A-Z0-9]+)",
    "Bank Account Number": "Bank Account(?:\\s*Number)?\\s*[:\\s]*([\\d\\s]+)",
    "IFSC Code": "IFSC(?:\\s*Code)?\\s*[:\\s]*([A-Z0-9]+)",
    "Amount in Words": "(?:Amount in Words|An amount of .*? words)\\s*(?:in words)?\\s*[:\\s]*([A-Za-z\\s\\-]+)\\s+([\\d,]+)",
    "Applicant Name": "(?:First\\/Sole Applicant|lame of First\\/Sole Applicant)\\s*[:\\s]*([A-Za-z\\s]*)",
    "Email ID": "(E-mail|Email)\\s*[:\\s]*([\\w\\.-]+@[\\w\\.-]+)",
    "PAN": "PAN\\s*[:\\s]*([A-Z]{5}\\d{4}[A-Z])"
}
